<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<title>Listado de Socios</title>
<frx:category/>
<frx:parameters>
  <frx:parm id="soc_cod_baja" label="Filtrar por Socio:" require="1" data_source="dbold/listaSociosParamSINOTODOS" type="select">ACTIVO</frx:parm>
  <frx:parm id="apellido" label="Filtrar por Apellido y/o Nombre:" type="textfield"/>
</frx:parameters>
<frx:docgen>
  <frx:doc type="xls"/>
  <frx:doc type="pdf"/>
</frx:docgen>
<frx:fields>
</frx:fields>
<frx:menu/>
<frx:cache/>
<style></style>
<frx:options input_format="full_html" skin="talleres"/></head>
<body>
<div id="totales_block" class="FrxTable" frx:block="dbold/listaSociosTotal">
  <p id="totales" frx:foreach="*"> Cantidad Total de Socios: <strong>{TOTAL_SOCIOS}</strong> filtrado por estado: {parm.soc_cod_baja}</p>
</div>  
<div id="_block" class="FrxTable catTable" frx:block="dbold/listaSocios">
  <table id="dt">
      <thead>
        <tr>
          <th>NÂº SOCIO</th>
          <th>APELLIDO</th>
          <th>DIRECCION</th>
          <th>TELEFONO</th>
          <th>DETALLES</th>
        </tr>
      </thead>
      <tbody>
        <tr id="io" frx:foreach="*">
          <td>{NRO_SOCIO}</td>
          <td>{APELLIDO}</td>
          <td>{DOMICILIO}</td>
          <td>{TELEFONO}</td>
          <td> <a class="colorbox-node" href="listaSociosDetalle?NRO_SOCIO={NRO_SOCIO}&amp;width=700px;&amp;height=800px;">VER DETALLE</a></td>
        </tr>
      </tbody>
    </table>
  </div>
</body>
</html>
